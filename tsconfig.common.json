{
    "buildOnSave": false,
    "compileOnSave": false,
    "compilerOptions": {
        "declaration": true,
        "lib": [
            "dom",
            "esnext"
        ],
        "module": "commonjs",
        "moduleResolution": "node",
        "noEmitOnError": true,
        "noImplicitAny": true,
        "noImplicitThis": true,
        "outDir": "./dist",
        "preserveConstEnums": true,
        "skipLibCheck": true,
        "sourceMap": true,
        "strict": true,
        "strictNullChecks": true,
        "strictPropertyInitialization": false,
        "target": "es5"
    }
}
